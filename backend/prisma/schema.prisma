generator client {
  provider = "prisma-client"
  engineType = "client"
  output   = "../src/generated/prisma"
  runtime                = "nodejs"
  moduleFormat           = "esm"
  generatedFileExtension = "ts"
  importFileExtension    = "ts"
}

datasource db {
  provider = "sqlite"
}

model User {
    id Int @id @default(autoincrement())
    name String
    email String @unique
}
